@page
@using System.Web
@using Core.Interfaces
@model Welcome

<h1>Welcome to @AppGlobal.CompanyName @AppGlobal.ProductName</h1>
<br />
<a href="#" load-modal="/Project/CompareProject?handler=new" load-modal-method="GET">New project</a>

<br />
<br />
<div>
    Recent projects:
    @foreach (var recent in Model.RecentProjects)
    {
        <br /> @:&bull;
        <a href="#" load-modal="/Project/CompareProject?handler=Load" load-modal-method="POST"
           load-modal-data="@HttpUtility.JavaScriptStringEncode(Json.Serialize(recent).ToString())">
            @recent
        </a>
    }
</div>
<br />
<br />

<label for="exampleInputEmail1">Load</label>
<input type="file">
